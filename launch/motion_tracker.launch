<launch>
  <group ns="$(arg NAMESPACE)">
    <group ns="$(arg MACHINE)">
      <group ns="camera">

        <arg name="PACKAGE_DIR" value="$(find motion_tracker)"/>

        <node pkg="motion_tracker" type="background_subtractor" name="background_subtractor" args="$(arg PACKAGE_DIR)" output="screen"/>

      </group>
    </group>
  </group>
</launch>

